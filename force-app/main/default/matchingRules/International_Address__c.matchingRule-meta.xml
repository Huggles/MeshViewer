<?xml version="1.0" encoding="UTF-8"?>
<MatchingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <matchingRules>
        <fullName>Based_on_Organization_or_Locality_or_Street_or_Postcode_or_HouseNumber</fullName>
        <booleanFilter>(1 AND 2 AND 3) OR (1 AND 2 AND 4)  OR (2 AND 3 AND 5) OR (2 AND 4)</booleanFilter>
        <description>Checks Duplicate if there are International Address with same
- Organization + Locality + Street or
- Organization + Locality + Postcode or
- Locality + Street + House Number or
- Locality + Postcode</description>
        <label>Based on Organization or Locality or Street or Postcode or HouseNumber</label>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>appsolutely__Organization__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>appsolutely__Locality__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>appsolutely__Street__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>appsolutely__Postcode__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>appsolutely__House_Number__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <ruleStatus>Active</ruleStatus>
    </matchingRules>
</MatchingRules>
