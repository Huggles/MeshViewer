<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Filled_By_Id__c</fullName>
    <description>Returns the id of either the person or the organisation, depending on which is filled in.</description>
    <externalId>false</externalId>
    <formula>IF( NOT(ISBLANK( Business_Person__c )) , Business_Person__c, Organisation__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Filled By Id</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
