<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
    <description>Account List Table</description>
    <isExposed>true</isExposed>
    <masterLabel>Account List Table</masterLabel>
    <targets>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="accountList" type="@salesforce/schema/Account[]" label="Account List" role="inputOnly"/>
            <property name="selectedResult" type="@salesforce/schema/Account" label="Selected Account" role="outputOnly"/>
            <property name="newAccount" type="@salesforce/schema/Account" label="New Account To Be Created" role="inputOnly"/>
            <property name="updateDuplicateAccount" type="Boolean" label="Update Duplicate Account" role="outputOnly"/>
            <property name="showCreateNewAccount" type="Boolean" label="Show Create New Account Button" role="inputOnly"/>
            <property name="cancelClicked" type="Boolean" label="Cancel Clicked" role="outputOnly"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
